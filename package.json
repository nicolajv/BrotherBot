{
  "name": "brotherbotv2",
  "version": "1.0.0",
  "description": "Oh boy here we go again",
  "main": "dist/app.js",
  "scripts": {
    "start": "npm run compile && node dist/infrastructure/server.js",
    "test": "npm run lint && npm run compile && jest",
    "compile": "tsc",
    "lint": "eslint ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "projects": [
      "dist/tests"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "dist/**"
    ],
    "coverageDirectory": "coverage",
    "clearMocks": true,
    "setupFiles": [
      "./dist/tests/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicolajv/BrotherBotV2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nicolajv/BrotherBotV2/issues"
  },
  "homepage": "https://github.com/nicolajv/BrotherBotV2#readme",
  "dependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/parser": "^4.4.0",
    "discord.js": "^12.3.1",
    "eslint": "^7.10.0",
    "express": "^4.17.1",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "request": "^2.88.2",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "jest": "^26.5.0"
  }
}
